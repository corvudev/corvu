{
  "name": "corvu",
  "version": "0.7.1",
  "private": false,
  "description": "Unstyled, accessible and customizable UI primitives for SolidJS",
  "keywords": [
    "solid",
    "solidjs",
    "headless",
    "accessible",
    "customizable",
    "primitives"
  ],
  "homepage": "https://corvu.dev",
  "bugs": {
    "url": "https://github.com/corvudev/corvu/issues",
    "email": "code@jasi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corvudev/corvu.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jasmin Noetzli",
    "email": "code@jasi.dev",
    "url": "https://github.com/GiyoMoon"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "solid": "./dist/*.jsx",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsup --watch",
    "lint": "eslint --max-warnings=0 ."
  },
  "dependencies": {
    "@corvu/accordion": "workspace:~",
    "@corvu/dialog": "workspace:~",
    "@corvu/disclosure": "workspace:~",
    "@corvu/drawer": "workspace:~",
    "@corvu/otp-field": "workspace:~",
    "@corvu/popover": "workspace:~",
    "@corvu/resizable": "workspace:~",
    "@corvu/tooltip": "workspace:~"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.19.1",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-solid": "^0.14.5",
    "prettier": "^3.4.2",
    "solid-js": "^1.9.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  },
  "peerDependencies": {
    "solid-js": "^1.8"
  }
}
