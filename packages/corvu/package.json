{
  "name": "corvu",
  "version": "0.0.0",
  "private": false,
  "description": "Unstyled, accessible and customizable primitives for SolidJS",
  "keywords": [
    "solid",
    "solidjs",
    "headless",
    "accessible",
    "customizable",
    "primitives"
  ],
  "homepage": "https://corvu.dev",
  "bugs": {
    "url": "https://github.com/corvudev/corvu/issues",
    "email": "code@jasi.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corvudev/corvu"
  },
  "license": "MIT",
  "author": {
    "name": "Jasmin Noetzli",
    "email": "code@jasi.dev",
    "url": "https://github.com/GiyoMoon"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/primitives/*/index.d.ts",
      "solid": "./dist/primitives/*/index.jsx",
      "default": "./dist/primitives/*/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm lint && tsup",
    "lint": "eslint --max-warnings=0 .",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@solid-primitives/memo": "^1.3.5",
    "@solid-primitives/props": "^3.1.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-solid": "^0.13.0",
    "prettier": "^3.1.0",
    "solid-js": "^1.8.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "solid-js": "^1.8"
  }
}
