{
  "name": "corvu",
  "version": "0.6.0",
  "private": false,
  "description": "Unstyled, accessible and customizable UI primitives for SolidJS",
  "keywords": [
    "solid",
    "solidjs",
    "headless",
    "accessible",
    "customizable",
    "primitives"
  ],
  "homepage": "https://corvu.dev",
  "bugs": {
    "url": "https://github.com/corvudev/corvu/issues",
    "email": "code@jasi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corvudev/corvu.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jasmin Noetzli",
    "email": "code@jasi.dev",
    "url": "https://github.com/GiyoMoon"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "solid": "./dist/*.jsx",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsup --watch",
    "lint": "eslint --max-warnings=0 ."
  },
  "dependencies": {
    "@corvu/accordion": "workspace:^",
    "@corvu/dialog": "workspace:^",
    "@corvu/disclosure": "workspace:^",
    "@corvu/drawer": "workspace:^",
    "@corvu/popover": "workspace:^",
    "@corvu/resizable": "workspace:^",
    "@corvu/tooltip": "workspace:^"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-solid": "^0.14.0",
    "prettier": "^3.2.5",
    "solid-js": "^1.8.17",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "solid-js": "^1.8"
  }
}
