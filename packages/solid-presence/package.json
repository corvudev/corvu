{
  "name": "solid-presence",
  "version": "0.1.7",
  "private": false,
  "description": "SolidJS utility that manages the presence of an element in the DOM while being aware of pending animations.",
  "keywords": [
    "solid",
    "solidjs",
    "presence",
    "animation"
  ],
  "homepage": "https://corvu.dev/docs/utilities/presence",
  "bugs": {
    "url": "https://github.com/corvudev/corvu/issues",
    "email": "code@jasi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corvudev/corvu.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jasmin Noetzli",
    "email": "code@jasi.dev",
    "url": "https://github.com/GiyoMoon"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules typedoc.json",
    "lint": "eslint --max-warnings=0 .",
    "typedoc": "rm -f typedoc.json && typedoc --json typedoc.json --entryPoints ./src/index.ts"
  },
  "dependencies": {
    "@corvu/utils": "workspace:~"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-solid": "^0.14.0",
    "prettier": "^3.3.2",
    "solid-js": "^1.8.17",
    "tsup": "^8.1.0",
    "typedoc": "0.25.13",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "solid-js": "^1.8"
  }
}
