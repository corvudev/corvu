{
  "name": "@corvu/utils",
  "version": "0.1.0",
  "private": false,
  "description": "Utilities for corvu",
  "keywords": [
    "solid",
    "solidjs",
    "headless",
    "accessible",
    "customizable",
    "primitives"
  ],
  "homepage": "https://corvu.dev",
  "bugs": {
    "url": "https://github.com/corvudev/corvu/issues",
    "email": "code@jasi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corvudev/corvu.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jasmin Noetzli",
    "email": "code@jasi.dev",
    "url": "https://github.com/GiyoMoon"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./scroll": {
      "types": "./dist/scroll.d.ts",
      "default": "./dist/scroll.js"
    },
    "./create-style": {
      "types": "./dist/createStyle.d.ts",
      "default": "./dist/createStyle.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "scroll": [
        "./dist/scroll.d.ts"
      ],
      "create-style": [
        "./dist/createStyle.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "lint": "eslint --max-warnings=0 ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-solid": "^0.13.1",
    "prettier": "^3.2.5",
    "solid-js": "^1.8.15",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "solid-js": "^1.8"
  }
}
